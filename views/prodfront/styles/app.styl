v-btn-style($material)
  &.v-btn__type1
    gradient(#12a1f5, #0d70de, left top, right  top)
    text-transform none
    color: $material.fg-color
  &.v-btn__type2
    mix-blend-mode: normal;
    background-color: #ffffff;
    border: 1px solid rgba(5, 146, 171, 0.40);
    box-sizing: border-box;
    color #000000
    font-size 12px
    box-shadow: 0px 4px 20px rgba(0, 0, 0, 0.15);
    border-radius: 8px;
    margin: 0px auto !important;
    min-width: auto;
    max-width: calc(100% - 14px);
    &.v-btn--active:before,
    &.v-btn:hover:before,
    &.v-btn:focus:before
      background-color #ffffff
    &.active
      background-color #0592AB
      color #ffffff
  &.v-btn-toolbar
    background: transparent
    text-transform none
    font-size 18px
    color: #fff
    &.v-btn--active:before,
    &.v-btn:hover:before,
    &.v-btn:focus:before {

      background-color: transparent;
    }
  &.v-btn-toolbar-right
    background: transparent
    text-transform none
    font-size 16px
    padding: 0px;
    color: #fff
    &.v-btn--active:before,
    &.v-btn:hover:before,
    &.v-btn:focus:before {
      background-color: transparent;
    }

    @media $display-breakpoints.md-and-up
      .v-btn__content
        border solid 2px transparent
        padding 4px 20px
      /*&.active
        .v-btn__content
          border solid 2px #fff
          padding 4px 20px*/

  &.v-btn__type3
    background #0592ab
    text-transform none
    color #ffffff
    border-radius 8px
    box-shadow: 0px 4px 20px rgba(0, 0, 0, 0.15);
    &.v-btn--outline {
      border: 1px solid #0592ab;
      background: transparent !important;
      -webkit-box-shadow: none;
      box-shadow: none;
      color: #0592ab !important;
    }
    &.v-btn--small {
      font-size: 15px;
      height: 32px;
      padding: 0 8px;
    }
    &.v-btn--active:before,
    &.v-btn:hover:before,
    &.v-btn:focus:before {
      background-color: transparent;
    }
  &.v-btn__type4
    background: transparent;
    text-transform: none;
    border: none;
    border-bottom: none
    color: #535353;
    font-size: 14px;
    border-radius: 6px;
    min-width: auto;
    padding: 0px;
    height: 20px;
    border-color #0592AB
    &.v-btn--active
      border-bottom: none;
    &.v-btn--active:before,
    &.v-btn:hover:before,
    &.v-btn:focus:before
      background-color transparent
    &.menuable__content__active
      background-color #0592AB
    &.active
      background-color rgba(4, 145, 170, 0.20)
  &.btn-action-table
    width 20px
    height 20px
    text-transform none
    color: $material.fg-color
    &.v-btn--outline {
      border: 1px solid red;
      background: transparent !important;
      -webkit-box-shadow: none;
      box-shadow: none;
      color: red !important;
    }
    &.v-btn--small {
      font-size: 10px;
      height: 18px;
      padding: 0 8px;
    }
    &.v-btn--active:before,
    &.v-btn:hover:before,
    &.v-btn:focus:before {
      background-color: transparent;
    }

  &.v-btn--disabled
    color: $material.fg-color !important;
    opacity: 0.5;
  &.v-btn__type_select_wallet
    color: $material.text.link !important;
    box-shadow: none;
    padding: 0px;
    margin: 0px;
    background: none;
    min-width: auto;
    width 100%
    border-radius: 0px;
    font-weight: bold;
