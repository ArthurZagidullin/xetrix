block-style-1($material)
  border-radius 7px
  border-color: rgba(39, 95, 158, 0.5);
  border-width 1px
  border-style solid
  box-shadow none
.address_currency {
  position: relative;
  &:before {
    content: '';
    position: absolute;
    top: 0px;
    right: 0px;
    z-index: 2;
    width: 100%;
    height: 70%;
    border-bottom: dotted 0.13rem #e8e8e8;
  }
  > div {
    background: #ffffff;
    z-index: 5;
    position: relative;
  }
}
.address_currency_style_2 {
  max-width: 100%;
  overflow: hidden;
  display: block;
  position: relative;
  &:before {
    content: '';
    position: absolute;
    top: 0px;
    right: 0px;
    z-index: 2;
    width: 100%;
    height: 90%;
    border-bottom: dotted 2px #bbb;
    gradient(rgba(255, 255, 255, 0), rgba(255, 255, 255, 100), left top, right  top, 0%, 80%)
  }
  > div {
    background: #ffffff;
    z-index: 5;
    position: relative;
  }
}
card-wallet($material)
  .header
    border-top-left-radius 8px
    border-top-right-radius 8px
    background #275F9E
    height 78px
    position relative
    color: $material.fg-color
    &:before
      border-top-left-radius: 8px;
      border-top-right-radius: 8px;
      border-bottom-left-radius: 0px;
      border-bottom-right-radius: 0px;
      content: '';
      position: absolute;
      width: calc(100% + 2px);
      height: 79px;
      background: #275f9e;
      z-index: 2;
      top: 0;
      left: 0;
      margin-left: -1px;
      margin-top: -1px;
