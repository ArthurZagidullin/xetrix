.container.fluid {
  max-width: 100%;
  flex: 1;
  width: 100%;
}
.container
  &.grid-list
    for $size, $gutter in $grid-gutters
      &-{$size}
        .layout:not(:only-child)
          margin: 0px;
.data
  table.v-table
    border-collapse: separate;
    height: 100%;
    thead, tbody
      td, th
        &:not(:nth-child(1)), &:first-child
          padding: 0 5px
    thead
      height 34px
      tr
        height 34px
        &.v-datatable__progress
          display none
      th
        font-weight: 400
        font-size: 16px
        transition: .3s $transition.swing
        white-space: nowrap
        user-select: none
        .v-icon
          margin-left: -16px;
        .v-input__control
          .v-icon
            margin-left: 0px;
        &.sortable
          pointer-events: auto

        > div
          width: 100%
    tbody
      tr
        transition: background $primary-transition
        will-change: background
      td, th
        height: 26px
        white-space: nowrap;
      td
        font-weight: 400
        font-size: 14px
    .input-group--selection-controls
      padding: 0
      .input-group__details
        display: none
      &.checkbox
        .v-icon
          left: 50%
          transform: translateX(-50%)
        .input-group--selection-controls__ripple
          left: 50%
          transform: translate(-50%, -50%)
    tfoot
      tr
        height: 26px
        td
          padding: 0 6px
  &.group-table
    min-height: 500px;
    background: #ffffff;
    position: relative;
    @media $display-breakpoints.sm-and-down
      min-height auto
      padding-bottom 28px
    table.v-table
      border-collapse: separate;
      tbody
        td
          border-width 1px 0px 0px 0px
          border-style solid
          border-color #f8fbfe
          font-weight 600
          position: relative
          .select-group-arrow
            position absolute
            right 20%;
            top 50%;
            margin-top -10px
            .v-icon
              font-size 20px
              &.mdi-chevron-left
                color: $blue.darken-0;
        tr
          background #ffffff
          td
            border-style solid
            border-color #ffffff
          &:hover,
          &.active
